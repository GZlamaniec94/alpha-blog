<h1>Create new article</h1>

<%= form_with scope: :article, url: articles_path, local: true do |f|%>
    <p>
        <strong> <%= f.label :title %></strong><br/>
        <%= f.text_field :title%>
    </p>
    <p>
        <strong><%= f.label :description%></strong><br/>
        <%= f.text_area :description%>
    </p>
    <p>
        <%= f.submit%>
    </p>
<%end%>
